{
  "routes": [
    {
      "route": "/assets/*",
      "headers": {
        "Cache-Control": "public, max-age=86400"
      }
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/assets*",
      "/static*"
    ]
  },
  "mimeTypes": {
    ".json": "application/json"
  }
}
